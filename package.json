{
  "name": "paperwork-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "start:tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --profile production --platform android",
    "build:development:android": "eas build --profile development --platform android",
    "build:preview:android": "eas build --profile preview --platform android",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --silent --coverage=true",
    "test:coverage:log": "jest --coverage=true",
    "test:all": "jest --watchAll --coverage=true"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/masked-view": "^0.1.11",
